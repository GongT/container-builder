	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "启动程序",
			"program": "${workspaceFolder}/loader.js",
			"skipFiles": ["<node_internals>/**", "**/node_modules/**"],
			"args": ["build"]
		},
		{
			"type": "node",
			"request": "attach",
			"name": "附加",
			"skipFiles": ["<node_internals>/**", "**/node_modules/**"],
			"outFiles": ["${workspaceFolder}/lib/**/*.js"]
		}
	]
}
